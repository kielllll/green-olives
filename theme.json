{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "appearanceTools": true,
    "color": {
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "palette": [
        {
          "color": "#FFFFFF",
          "name": "Base",
          "slug": "base"
        },
        {
          "color": "#111111",
          "name": "Contrast",
          "slug": "contrast"
        },
        {
          "color": "#FFEE58",
          "name": "Accent 1",
          "slug": "accent-1"
        },
        {
          "color": "#F6CFF4",
          "name": "Accent 2",
          "slug": "accent-2"
        },
        {
          "color": "#503AA8",
          "name": "Accent 3",
          "slug": "accent-3"
        },
        {
          "color": "#686868",
          "name": "Accent 4",
          "slug": "accent-4"
        },
        {
          "color": "#FBFAF3",
          "name": "Accent 5",
          "slug": "accent-5"
        },
        {
          "color": "color-mix(in srgb, currentColor 20%, transparent)",
          "name": "Accent 6",
          "slug": "accent-6"
        }
      ]
    },
    "layout": {
      "contentSize": "645px",
      "wideSize": "1340px"
    },
    "spacing": {
      "defaultSpacingSizes": false,
      "spacingSizes": [
        {
          "name": "Tiny",
          "size": "10px",
          "slug": "20"
        },
        {
          "name": "X-Small",
          "size": "20px",
          "slug": "30"
        },
        {
          "name": "Small",
          "size": "30px",
          "slug": "40"
        },
        {
          "name": "Regular",
          "size": "clamp(30px, 5vw, 50px)",
          "slug": "50"
        },
        {
          "name": "Large",
          "size": "clamp(30px, 7vw, 70px)",
          "slug": "60"
        },
        {
          "name": "X-Large",
          "size": "clamp(50px, 7vw, 90px)",
          "slug": "70"
        },
        {
          "name": "XX-Large",
          "size": "clamp(70px, 10vw, 140px)",
          "slug": "80"
        }
      ],
      "units": ["%", "px", "em", "rem", "vh", "vw"]
    }
  }
}
